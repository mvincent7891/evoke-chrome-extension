{
  "short_name": "Evoke",
  "name": "Evoke Chrome Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
      "default_popup": "index.html"
  },
  "permissions": [
    "activeTab", "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"],
      "js": ["contentScript.js"]
    }
  ]
}
